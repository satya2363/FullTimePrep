			String s = "eabcb";
		 boolean[][] result = new boolean[s.length()][s.length()];
	        
	        for(int i=0;i<s.length();i++)
	        {
	            for(int j=s.length()-1;j>=0;j--)
	            {
	                if(s.charAt(i) == s.charAt(j) )
	                result[i][j] = true;
	                
	                else
	                result[i][j] = false;
	                
	            }
	        }
	        
